{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "ses:SendEmail",
                "ses:SendRawEmail",
                "ses:SendBulkTemplatedEmail"
            ],
            "Resource": [
                "arn:aws:ses:ap-south-1:123123123123:identity/example.com.com",
                "arn:aws:ses:ap-south-1:123123123123:identity/hello@example.com.com"
            ],
            "Condition": {
                "StringEquals": {
                    "ses:FromAddress": "hello@example.com.com"
                }
            }
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": "ses:SendTemplatedEmail",
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "ses:FromAddress": "hello@example.com.com"
                }
            }
        }
    ]
}
